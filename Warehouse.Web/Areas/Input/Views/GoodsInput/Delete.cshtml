@model Warehouse.Web.ViewModel.Input.GoodsInputVMs.GoodsInputVM

<wt:form vm="@Model">
	<wt:quote>确定要删除这条数据么：</wt:quote>
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:display field="Entity.InputTime" />
<wt:display field="Entity.Supplier.SupplierName" />
<wt:display field="Entity.GoodsName" />
<wt:display field="Entity.GoodsDesc" />
<wt:display field="Entity.Producer" />
<wt:display field="Entity.Specification" />
<wt:display field="Entity.BatchNumber" />
<wt:display field="Entity.ApprovalNo" />
<wt:display field="Entity.SellingPrice" />
<wt:display field="Entity.InputNumber" />
<wt:display field="Entity.WarningValue" />
<wt:display field="Entity.ActiveFlag" />
<wt:display field="Entity.PhotoId" />
</wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align=" AlignEnum.Right">
        <wt:submitbutton theme=" ButtonThemeEnum.Warm" text="删除"/>
        <wt:closebutton />
    </wt:row>
</wt:form>