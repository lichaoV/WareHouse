@model Warehouse.Web.ViewModel.Input.GoodsInputVMs.GoodsInputListVM

<wt:searchpanel vm="@Model" reset-btn="true">
<wt:row items-per-row="ItemsPerRowEnum.Three">
<wt:combobox field="Searcher.SupplierId" items="Searcher.AllSuppliers" empty-text="全部" />
<wt:combobox field="Searcher.GoodsInfoId" items="Searcher.AllGoodsInfos" empty-text="全部" />
