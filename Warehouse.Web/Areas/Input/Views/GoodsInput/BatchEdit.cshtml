@model Warehouse.Web.ViewModel.Input.GoodsInputVMs.GoodsInputBatchVM

<wt:form vm="@Model">
    <div style="margin-bottom:10px">批量修改以下数据: </div>
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:combobox field="LinkedVM.SupplierId" items="LinkedVM.AllSuppliers"/>
<wt:textbox field="LinkedVM.Producer" />
<wt:textbox field="LinkedVM.BatchNumber" />
<wt:textbox field="LinkedVM.ApprovalNo" />
</wt:row>
    <wt:hidden field="Ids" />
    <wt:grid vm="ListVM" use-local-data="true" height="300"  hidden-checkbox="true" hidden-panel="true"/>
    <wt:row align=" AlignEnum.Right">
        <wt:submitbutton />
        <wt:closebutton />
    </wt:row>
</wt:form>