@model Warehouse.Web.ViewModel.Output.SaleOutputVMs.SaleOutputVM

<wt:form vm="@Model">
	<wt:quote>确定要删除这条数据么：</wt:quote>
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:display field="Entity.SaleTime" />
<wt:display field="Entity.Customer.CustomerName" />
<wt:display field="Entity.GoodsInfo.GoodsName" />
<wt:display field="Entity.SaleNumber" />
<wt:display field="Entity.SalePrice" />
<wt:display field="Entity.PayType" />
<wt:display field="Entity.SaleRemark" />
</wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align=" AlignEnum.Right">
        <wt:submitbutton theme=" ButtonThemeEnum.Warm" text="删除"/>
        <wt:closebutton />
    </wt:row>
</wt:form>