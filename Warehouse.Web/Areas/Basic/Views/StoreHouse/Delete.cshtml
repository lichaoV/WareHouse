@model Warehouse.Web.ViewModel.Basic.StoreHouseVMs.StoreHouseVM

<wt:form vm="@Model">
	<wt:quote>确定要删除这条数据么：</wt:quote>
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:display field="Entity.WarehouseCode" />
<wt:display field="Entity.WarehouseAddress" />
<wt:display field="Entity.ContractName" />
<wt:display field="Entity.ContractPhone" />
<wt:display field="Entity.Remark" />
</wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align=" AlignEnum.Right">
        <wt:submitbutton theme=" ButtonThemeEnum.Warm" text="删除"/>
        <wt:closebutton />
    </wt:row>
</wt:form>