@model Warehouse.Web.ViewModel.Basic.SalesmanVMs.SalesmanVM

<wt:form vm="@Model">
	<wt:quote>确定要删除这条数据么：</wt:quote>
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:display field="Entity.SalesmanName" />
<wt:display field="Entity.SalesmanCode" />
<wt:display field="Entity.SalesmanPhone" />
<wt:display field="Entity.EntryTime" />
<wt:display field="Entity.QuitTime" />
<wt:display field="Entity.Sex" />
<wt:display field="Entity.ActiveFlag" />
<wt:display field="Entity.PhotoId" />
</wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align=" AlignEnum.Right">
        <wt:submitbutton theme=" ButtonThemeEnum.Warm" text="删除"/>
        <wt:closebutton />
    </wt:row>
</wt:form>